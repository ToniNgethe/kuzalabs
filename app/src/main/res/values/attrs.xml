<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

    <declare-styleable name="Fingerprint">
        <attr name="fingerprintScanningColor" format="color" />
        <attr name="fingerprintSuccessColor" format="color" />
        <attr name="fingerprintErrorColor" format="color" />

        <attr name="circleScanningColor" format="color" />
        <attr name="circleSuccessColor" format="color" />
        <attr name="circleErrorColor" format="color" />
    </declare-styleable>


    <declare-styleable name="CustomEditText">
        <attr name="drawableStartCompat" format="reference" />
        <attr name="drawableEndCompat" format="reference" />
        <attr name="drawableTopCompat" format="reference" />
        <attr name="drawableBottomCompat" format="reference" />
    </declare-styleable>


</resources>